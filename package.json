{
  "name": "remotequery-ts-mysql",
  "version": "0.0.14",
  "description": "Remotequery MySql Driver for TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.ts'  --exit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonibuenter/remotequery-ts-mysql.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "sql",
    "addon",
    "remotequery"
  ],
  "author": "Toni A. Buenter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tonibuenter/remote-query-js-mysql/issues"
  },
  "homepage": "https://github.com/tonibuenter/remote-query-ts-mysql#readme",
  "dependencies": {
    "camelcase": "^5.3.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "mocha": "^9.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
